LucyMissilePrimary
{
ejector
  {
    particle
    {
      displacement		0 0 0 ~25

      velocityType		cent
      velocityDir		linear
      velocityMagnitude		500
      velocity			0 0 1 ~30

      //accelerationType		static_transform
      accelerationType		cent
      //accelerationDir   	linear
      accelerationDir   	point
      accelerationMagnitude	50000
      //acceleration		0 -1 0 0
      accelerationPoint -100 0 0 ~0

      parentVelocityFraction 0.0

      radius 0.0 1.0 -
      alpha 0.0 1.0 1.0
      bounce 0.0
      rotation 0.0 0.0 -
      lifeTime 1000.0

      childTrailSystem		lucymuzzletrail
    }
    count infinite
    delay 0
    period 175 - ~0%
  }
  ejector
  {
    particle
    {
      //shader sync gfx/blaster/orange_particle

      displacement		0 0 0 ~3.0

      velocityType		static
      velocityDir		linear
      velocityMagnitude		500
      velocity			0 0 1 ~360

      radius 0 0.5 0.5
      alpha 0 1.0 1.0
      bounce cull
      rotation 0 0 -
      lifeTime 550

      onDeathSystem LucyPrimaryExtras

    }
    count infinite
    delay 0
    period 50 - ~0%
  }
}
