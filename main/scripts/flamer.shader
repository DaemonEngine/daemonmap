models/weapons/flamer/weapongrip
{
	cull disable
	nopicmip
	surfaceparm alphashadow
	{
		map models/weapons/flamer/weapongrip.tga
		alphaFunc GE128
		depthWrite
		rgbGen lightingDiffuse
	}
}

gfx/weapons/flamersmoke
{
  cull none
  entityMergable
  {
    map gfx/weapons/flamersmoke.tga
    blendFunc blend
    rgbGen vertex
    alphaGen vertex
  }
}

gfx/weapons/flamerhaze
{
  cull none
  entityMergable
  implicitMapGL1 gfx/transparent.png
  {
    stage heathazeMap
    deformMagnitude 4.0
    map gfx/weapons/flamerhaze_normal.tga
  }
}

